{
	"名称": "群服互通",
	"描述": "群服互通 游戏内可以看到Q群聊天，Q群也能看到服务器聊天",
	"是否禁用": false,
	"版本": "0.0.1",
	"来源": "Built-In",
	"配置": {
		"CQHTTP正向Websocket代理地址": "127.0.0.1:6700",
		"Q群消息格式化模版": "[groupName]:[QQUserName]:[msg]",
		"不要转发机器人的消息": true,
		"只转发聊天消息": true,
		"屏蔽其他群的消息": false,
		"游戏内可以听到QQ消息的玩家的选择器": "@a",
		"游戏消息格式化模版": "[player]: [msg]",
		"链接的Q群": {
			"1群": [群号]
		}
	}
}
